   "source": [
    "## 4. Manufacturing Tolerance Analysis\n",
    "\n",
    "Analyze model performance under different manufacturing tolerance requirements."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze different tolerance levels\n",
    "tolerances = [0.5, 1.0, 2.0, 3.0, 5.0]  # Percentage tolerance levels\n",
    "spec_ranges = [(70, 95), (75, 90), (80, 95)]  # Different spec ranges\n",
    "\n",
    "tolerance_analysis = []\n",
    "\n",
    "print(\"=== Manufacturing Tolerance Analysis ===\")\n",
    "for spec_low, spec_high in spec_ranges:\n",
    "    print(f\"\\nSpec Range: {spec_low}% - {spec_high}%\")\n",
    "    for tolerance in tolerances:\n",
    "        # Calculate metrics with current tolerance\n",
    "        metrics = YieldRegressionPipeline.compute_metrics(\n",
    "            y, y_pred, \n",
    "            tolerance=tolerance,\n",
    "            spec_low=spec_low,\n",
    "            spec_high=spec_high,\n",
    "            cost_per_unit=1.0\n",
    "        )\n",
    "        \n",
    "        tolerance_analysis.append({\n",
    "            'Spec_Range': f'{spec_low}-{spec_high}%',\n",
    "            'Tolerance': tolerance,\n",
    "            'PWS': metrics['PWS'],\n",
    "            'Within_Tolerance': np.mean(np.abs(residuals) <= tolerance),\n",
    "            'Estimated_Loss': metrics['Estimated_Loss']\n",
    "        })\n",
    "        \n",
    "        print(f\"  Tolerance ±{tolerance}%: PWS={metrics['PWS']:.1%}, \"\n",
    "              f\"Within_Tol={np.mean(np.abs(residuals) <= tolerance):.1%}\")\n",
    "\n",
    "tolerance_df = pd.DataFrame(tolerance_analysis)\n",
    "print(\"\\n=== Tolerance Analysis Summary ===\")\n",
    "print(tolerance_df.round(3))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Visualize tolerance analysis\n",
    "fig, axes = plt.subplots(1, 2, figsize=(15, 5))\n",
    "\n",
    "# PWS vs Tolerance for different spec ranges\n",
    "ax1 = axes[0]\n",
    "for spec_range in tolerance_df['Spec_Range'].unique():\n",
    "    data = tolerance_df[tolerance_df['Spec_Range'] == spec_range]\n",
    "    ax1.plot(data['Tolerance'], data['PWS'], 'o-', linewidth=2, label=f'Spec: {spec_range}')\n",
    "\n",
    "ax1.set_xlabel('Tolerance Level (%)')\n",
    "ax1.set_ylabel('PWS (Prediction Within Spec)')\n",
    "ax1.set_title('PWS vs Manufacturing Tolerance')\n",
    "ax1.legend()\n",
    "ax1.grid(True, alpha=0.3)\n",
    "\n",
    "# Within Tolerance Rate\n",
    "ax2 = axes[1]\n",
    "tolerance_75_90 = tolerance_df[tolerance_df['Spec_Range'] == '75-90%']\n",
    "ax2.plot(tolerance_75_90['Tolerance'], tolerance_75_90['Within_Tolerance'], 'o-', \n",
    "         linewidth=2, color='green', label='Predictions Within Tolerance')\n",
    "ax2.axhline(y=0.8, color='red', linestyle='--', alpha=0.7, label='80% Target')\n",
    "ax2.axhline(y=0.9, color='orange', linestyle='--', alpha=0.7, label='90% Target')\n",
    "ax2.set_xlabel('Tolerance Level (%)')\n",
    "ax2.set_ylabel('Fraction Within Tolerance')\n",
    "ax2.set_title('Prediction Accuracy vs Tolerance (Spec: 75-90%)')\n",
    "ax2.legend()\n",
    "ax2.grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Find optimal tolerance for 80% accuracy\n",
    "target_accuracy = 0.8\n",
    "for tolerance in tolerances:\n",
    "    accuracy = np.mean(np.abs(residuals) <= tolerance)\n",
    "    if accuracy >= target_accuracy:\n",
    "        print(f\"\\n=== Optimal Operating Point ===\")\n",
    "        print(f\"For {target_accuracy:.0%} prediction accuracy: ±{tolerance}% tolerance required\")\n",
    "        break"
   ]
  },
  {
